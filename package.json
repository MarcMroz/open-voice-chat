{
  "name": "ses-chat",
  "version": "1.0.0",
  "description": "Lightweight Dockerized Voice Chat",
  "main": "server.js",
  "scripts": {
    "prestart": "node -e \"const fs=require('fs');if(!fs.existsSync('node_modules'))require('child_process').execSync('npm install --no-audit --no-fund',{stdio:'inherit'});\"",
    "start": "node server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "i18next": "^23.16.8",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^2.6.2",
    "peer": "^1.0.0",
    "socket.io": "^4.7.2"
  }
}
